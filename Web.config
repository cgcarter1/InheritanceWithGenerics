<configuration>
  <connectionStrings>
    <add name="code_sample" connectionString="Data Source=(LocalDb)\MSSQLLocalDB;Initial Catalog=Database;AttachDbFilename=|DataDirectory|\Database.mdf;Integrated Security=SSPI"
      providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.5.2">
    </compilation>
    <httpRuntime targetFramework="4.5.2"/>
    <authentication mode="Windows" />
    <authorization>
      <allow users="*"/>
    </authorization>
  </system.web>
</configuration>